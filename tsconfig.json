{
    "compilerOptions": {
        "alwaysStrict": true,
        "listEmittedFiles": true,
        "module": "commonJS",
        "newLine": "CRLF",
        "removeComments": false,
        "outDir": "./",
        "preserveConstEnums": true,
        "target":"es6"
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}